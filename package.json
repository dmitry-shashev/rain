{
  "name": "rain",
  "version": "0.0.32",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "pnpm prisma-generate",
    "dev": "nx run-many --target=serve",
    "build": "nx run-many --target=build",
    "prod": "pnpm build && nx run-many --target=serve --prod",
    "e2e": "nx e2e ui-e2e",
    "lint": "nx affected --target=lint --base=origin/master --parallel=5",
    "lint-styles": "nx affected --target=stylelint --base=origin/master --parallel=5",
    "test": "nx affected --target=test --base=origin/master --parallel=5",
    "tsc": "nx affected --target=tsc --base=origin/master --parallel=5",
    "prettier-format": "prettier --write . && git add --all",
    "generate-rtk-frontend": "rtk-query-codegen-openapi ./libs/store/src/lib/api/auto-generated/openapi-config-v1.ts && eslint --fix ./libs/store/src/lib/api/auto-generated/auto-generated.api.ts && prettier --write ./libs/store/src/lib/api/auto-generated/auto-generated.api.ts",
    "prisma-generate": "env-cmd -f .local.env prisma generate",
    "prisma-migrate": "env-cmd -f .local.env prisma migrate dev",
    "prisma-pull": "env-cmd -f .local.env prisma db pull",
    "prisma-push": "env-cmd -f .local.env prisma db push",
    "prisma-seed": "env-cmd -f .local.env prisma db seed",
    "prisma-studio": "env-cmd -f .local.env prisma studio",
    "prisma-validate": "env-cmd -f .local.env prisma validate"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@nx-tools/nx-prisma": "4.0.3",
    "@nx/cypress": "16.6.0",
    "@nx/eslint-plugin": "16.6.0",
    "@nx/jest": "16.6.0",
    "@nx/linter": "16.6.0",
    "@nx/nest": "16.6.0",
    "@nx/node": "16.6.0",
    "@nx/react": "16.6.0",
    "@nx/vite": "16.6.0",
    "@nx/webpack": "16.6.0",
    "@nx/workspace": "16.6.0",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@swc/jest": "0.2.20",
    "@testing-library/react": "13.4.0",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.4",
    "@types/node": "18.11.9",
    "@types/passport-jwt": "3.0.8",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-c8": "0.32.4",
    "@vitest/ui": "0.32.4",
    "cypress": "12.17.3",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jsdom": "22.1.0",
    "nx": "16.6.0",
    "nx-cloud": "16.3.0",
    "nx-stylelint": "15.0.0",
    "postcss-scss": "4.0.6",
    "prettier": "^2.6.2",
    "sass": "^1.55.0",
    "stylelint": "^15.0.0",
    "stylelint-config-standard": "^30.0.0",
    "stylelint-prettier": "2.0.0",
    "swc-loader": "0.1.15",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vite-plugin-dts": "2.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.4"
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "engines": {
    "node": ">=18.12.1",
    "pnpm": ">=7.27.0"
  },
  "packageManager": "pnpm@7.27.0",
  "dependencies": {
